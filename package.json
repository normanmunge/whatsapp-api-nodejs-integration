{
  "name": "whatsapp-nodejs",
  "version": "1.0.0",
  "description": "A simple nodejs chatbot that uses whatsapp cloud api",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "dev": "NODE_ENV=dev nodemon index.js",
    "staging": "NODE_ENV=staging nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:dev": "npx knex migrate:latest --knexfile db/knexfile.js --env development",
    "migrate:staging": "npx knex migrate:latest --knexfile db/knexfile.js --env staging",
    "migrate:prod": "npx knex migrate:latest --knexfile db/knexfile.js --env production",
    "migrate:rollback": "npx knex migrate:rollback --env development"
  },
  "author": "Norman Munge",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "http-errors": "^2.0.0",
    "http-proxy-middleware": "^2.0.6",
    "knex": "^3.0.1",
    "morgan": "^1.10.0",
    "needle": "^3.2.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
